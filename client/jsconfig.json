{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "jsx": "preserve",
        "baseUrl": "./",
        "paths": {
            "App/*": ["./app/App/*"],
            "Component/*": ["./app/Component/*"],
            "Controller/*": ["./app/Controller/*"],
            "locals/*": ["./app/locals/*"],
            "Constant/*": ["./app/Constant/*"],
            "Redux/*": ["./app/Redux/*"],
            "Styles/*": ["./app/Styles/*"],
            "Static/*": ["./app/Static/*"],
            "Utils/*": ["./app/Utils/*"],
            "JsonServer/*": ["./app/jsonServer/*"],
            "Root/*": ["./app/*"]
        }
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "dist",
        "tmp"
    ]
}